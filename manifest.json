{
  "name": "Corgi-Break",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "It's time for a Corgi Break.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Corgi-Break",
    "default_popup": "main.html"
  },
  "permissions": [
    "storage",
    "http://*.reddit.com/*",
    "http://fonts.googleapis.com/*"
  ]
}